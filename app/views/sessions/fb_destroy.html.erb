<script type="text/javascript">
  $(function() {
    setTimeout(function() {
      window.location = '<%= logout_url %>';
    }, 10000);

    App.Tasks.addTask( function() {
      FB.logout(function() {
        $.get('<%= facebook_connect_update_url %>', {'sid': ''}, function() {
          window.location = '<%= logout_url %>';
        });
      });
    });
  });
</script>
<%= t('label.logout_in_progress') %>